# Dify API Configuration
# Copy this file to .env and fill in your actual values

# Dify API Base URL (default: https://api.dify.ai/v1)
VITE_DIFY_API_BASE_URL=https://api.dify.ai/v1

# Your Dify Dataset ID
VITE_DIFY_DATASET_ID=your-dataset-id-here

# Your Dify API Bearer Token (for dataset operations)
VITE_DIFY_API_KEY=your-dataset-api-key-here

# Your Dify Chat API Bearer Token (for chatbot operations)
VITE_DIFY_CHAT_API_KEY=your-chat-api-key-here

# Chat user identifier (default: default-user)
VITE_DIFY_CHAT_USER=default-user